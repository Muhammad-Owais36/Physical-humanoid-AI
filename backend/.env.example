# RAG Agent Service Configuration

# OpenAI API key for agent orchestration
OPENAI_API_KEY=your_openai_api_key_here

# Qdrant Cloud configuration
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_URL=your_qdrant_cloud_url_here

# Qdrant collection name (default: rag_embeddings from previous features)
QDRANT_COLLECTION_NAME=rag_embeddings

# OpenAI assistant configuration
OPENAI_ASSISTANT_ID=your_assistant_id_here

# Model configuration
MODEL_NAME=gpt-4-1106-preview

# Retrieval configuration
RETRIEVAL_THRESHOLD=0.5
MAX_CONTEXT_CHUNKS=5
MAX_TOKENS=1000
TEMPERATURE=0.7

# Service configuration
HOST=0.0.0.0
PORT=8000

# Validation-specific configuration (from previous features)
VALIDATION_LIMIT=10
RELEVANCE_THRESHOLD=0.7